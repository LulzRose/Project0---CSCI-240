#include <iostream>
#include <windows.h>
using namespace std;

void pushX(int num){
    for(int i=0;i<num;i++)
        cout<<" ";
}
int main() {
    for(int j=0;j<500;j++){
        system("CLS");
        pushX(j);
        cout<<"Hello World!";
        Sleep(50);
    }
	return 0;	
}

